aliasdef cnn
	classalias
		augmentedImageSource -> augmentedImageDatastore;
        nnet.internal.cnn.FourDArrayMiniBatchDatasource -> nnet.internal.cnn.FourDArrayMiniBatchDatastore;
        nnet.internal.cnn.ImageDatastoreMiniBatchDatasource -> nnet.internal.cnn.ImageDatastoreMiniBatchDatastore;
